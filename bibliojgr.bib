	keywords = {Culture, Humans, Hypnosis, Neurosciences}
}

@article{landry_hypnosis_2015,
	title = {Hypnosis and imaging of the living human brain},
	volume = {57},
	issn = {0002-9157},
	doi = {10.1080/00029157.2014.978496},
	abstract = {Over more than two decades, studies using imaging techniques of the living human brain have begun to explore the neural correlates of hypnosis. The collective findings provide a gripping, albeit preliminary, account of the underlying neurobiological mechanisms involved in hypnotic phenomena. While substantial advances lend support to different hypotheses pertaining to hypnotic modulation of attention, control, and monitoring processes, the complex interactions among the many mediating variables largely hinder our ability to isolate robust commonalities across studies. The present account presents a critical integrative synthesis of neuroimaging studies targeting hypnosis as a function of suggestion. Specifically, hypnotic induction without task-specific suggestion is examined, as well as suggestions concerning sensation and perception, memory, and ideomotor response. The importance of carefully designed experiments is highlighted to better tease apart the neural correlates that subserve hypnotic phenomena. Moreover, converging findings intimate that hypnotic suggestions seem to induce specific neural patterns. These observations propose that suggestions may have the ability to target focal brain networks. Drawing on evidence spanning several technological modalities, neuroimaging studies of hypnosis pave the road to a more scientific understanding of a dramatic, yet largely evasive, domain of human behavior.},
	pages = {285--313},
	number = {3},
	journaltitle = {The American Journal of Clinical Hypnosis},
	shortjournal = {Am J Clin Hypn},
	author = {Landry, Mathieu and Raz, Amir},
	year = {2015},
	pmid = {25928680},
	keywords = {Brain, functional magnetic resonance imaging, Functional Neuroimaging, Humans, Hypnosis, Neuroimaging, positron emission tomography}
}

@article{gandhi_does_2005,
	title = {Does 'hypnosis' by any other name smell as sweet? The efficacy of 'hypnotic' inductions depends on the label 'hypnosis'},
	volume = {14},
	issn = {1053-8100},
	doi = {10.1016/j.concog.2004.12.004},
	shorttitle = {Does 'hypnosis' by any other name smell as sweet?},
	abstract = {Hypnosis is associated with profound changes in conscious experience and is increasingly used as a cognitive tool to explore neuropsychological processes. Studies of this sort typically employ suggestions following a hypnotic induction to produce changes in perceptual experience and motor control. It is not clear, however, to what extent the induction procedure serves to facilitate suggested phenomena. This study investigated the effect on suggestibility of (a) a hypnotic induction and (b) labelling that procedure 'hypnosis.' Suggestibility of participants was tested before and after an adapted hypnotic procedure, which was either labelled as 'hypnosis' or as 'relaxation.' The hypnotic procedure produced a modest increase in suggestibility when it was called 'relaxation,' but a very significant increase if it was labelled 'hypnosis.' The results are important for both clinical and experimental applications and indicate that labelling an induction procedure 'hypnosis' is an important determinant of subsequent responses to suggestion.},
	pages = {304--315},
	number = {2},
	journaltitle = {Consciousness and Cognition},
	shortjournal = {Conscious Cogn},
	author = {Gandhi, Balaganesh and Oakley, David A.},
	year = {2005},
	pmid = {15950884},
	keywords = {Adolescent, Adult, Consciousness, Female, Humans, Hypnosis, Male, Suggestion, Surveys and Questionnaires}
}

@article{kihlstrom_neuro-hypnotism:_2013,
	title = {Neuro-hypnotism: prospects for hypnosis and neuroscience},
	volume = {49},
	issn = {1973-8102},
	doi = {10.1016/j.cortex.2012.05.016},
	shorttitle = {Neuro-hypnotism},
	abstract = {The neurophysiological substrates of hypnosis have been subject to speculation since the phenomenon got its name. Until recently, much of this research has been geared toward understanding hypnosis itself, including the biological bases of individual differences in hypnotizability, state-dependent changes in cortical activity occurring with the induction of hypnosis, and the neural correlates of response to particular hypnotic suggestions (especially the clinically useful hypnotic analgesia). More recently, hypnosis has begun to be employed as a method for manipulating subjects' mental states, both cognitive and affective, to provide information about the neural substrates of experience, thought, and action. This instrumental use of hypnosis is particularly well-suited for identifying the neural correlates of conscious and unconscious perception and memory, and of voluntary and involuntary action.},
	pages = {365--374},
	number = {2},
	journaltitle = {Cortex; a Journal Devoted to the Study of the Nervous System and Behavior},
	shortjournal = {Cortex},
	author = {Kihlstrom, John F.},
	year = {2013},
	pmid = {22748566},
	pmcid = {PMC3528837},
	keywords = {Analgesia, Electroencephalography, Frontal lobe, Functional Laterality, Humans, Hypnosis, Neurosciences, Suggestion}
}

