	keywords = {Animals, Automatic Data Processing, Brain, Brain Mapping, Cats, Dendrites, Electroencephalography, Humans, Models, Biological, Neurons, Nonlinear Dynamics, Spectrum Analysis, Thermodynamics},
	pages = {257--265}
}

