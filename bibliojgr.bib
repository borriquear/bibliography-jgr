@article{joffily_emotional_2013,
	title = {Emotional {Valence} and the {Free}-{Energy} {Principle}},
	volume = {9},
	issn = {1553-7358},
	url = {http://journals.plos.org/ploscompbiol/article?id=10.1371/journal.pcbi.1003094},
	doi = {10.1371/journal.pcbi.1003094},
	abstract = {Author Summary  Emotion plays a crucial role in the adaptation of humans and other animals to changes in their world. Nevertheless, emotion has been neglected in Bayesian models of learning in non-stationary environments. The free-energy principle has recently been proposed as a unified account of learning, perception and action in biological agents. In this paper, we propose a formal definition of emotional valence (i.e., the positive and negative character of emotion) in terms of the rate of change of free-energy or, under some simplifying assumptions, of prediction error over time. This formalization leads to a straightforward and simple meta-learning scheme that accounts for the complex and reciprocal interaction between cognition and emotion. We instantiate this scheme with an emotional agent who is able to dynamically assign emotional valence to every new state of the world that is visited and to experience basic forms of emotion. Crucially, our hypothetical agent uses emotional valence to dynamically adapt to unexpected changes in the world. The proposed scheme is very general in the sense that it is not tied to any particular generative model of sensory inputs.},
	number = {6},
	urldate = {2016-05-12},
	journal = {PLOS Comput Biol},
	author = {Joffily, Mateus and Coricelli, Giorgio},
	month = jun,
	year = {2013},
	keywords = {Agent-based modeling, Emotions, Fear, Happiness, Human learning, Learning, Perceptual learning, Sensory perception},
	pages = {e1003094},
	file = {Full Text PDF:C\:\\Users\\Jaime\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\yg1f2pnd.default\\zotero\\storage\\69MW44R7\\Joffily and Coricelli - 2013 - Emotional Valence and the Free-Energy Principle.pdf:application/pdf;Snapshot:C\:\\Users\\Jaime\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\yg1f2pnd.default\\zotero\\storage\\JIF9AWK3\\article.html:text/html}
