	pages = {137--179},
	file = {Snapshot:C\:\\Users\\Jaime\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\yg1f2pnd.default\\zotero\\storage\\EK6GRKTU\\137.html:text/html}
}
