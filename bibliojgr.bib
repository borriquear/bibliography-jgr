	year = {2010},
	pmid = {20201851},
	keywords = {Animals, Brain, Cocaine, Cocaine-Related Disorders, Dopamine, Electrophysiological Phenomena, Humans, Neurons, Reward, Street Drugs, Substance-Related Disorders},
	pages = {140--147}
}

