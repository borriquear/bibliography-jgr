	year = {2014},
	pmid = {24647944},
	keywords = {Nucleus Accumbens, opioid},
	pages = {4239--4250},
	file = {Snapshot:/Users/jagomez/Library/Application Support/Firefox/Profiles/4otj4fuq.default/zotero/storage/CF72EB3E/4239.html:text/html}
