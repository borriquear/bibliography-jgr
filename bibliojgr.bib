	keywords = {Adolescent, Adult, Alpha Rhythm, Beta Rhythm, Biological Clocks, Brain Mapping, cerebral cortex, Computer Simulation, Electrodes, Implanted, Epilepsy, Exploratory Behavior, Female, Goals, Humans, Imaging, Three-Dimensional, Learning, Male, Nerve Net, Photic Stimulation, Psychomotor Performance, Space Perception, Theta Rhythm, Video Games},
	pages = {4726--4736}
}

@article{giaume_control_1996,
