	pages = {2716--2727},
	file = {Full Text PDF:/Users/jagomez/Library/Application Support/Firefox/Profiles/4otj4fuq.default/zotero/storage/NCVTPEE7/Takesian et al. - 2010 - Presynaptic GABAB Receptors Regulate Experience-De.pdf:application/pdf;Snapshot:/Users/jagomez/Library/Application Support/Firefox/Profiles/4otj4fuq.default/zotero/storage/IBATGFDG/2716.html:text/html}
