	year = {2002},
	note = {{PMID:} 11818563},
	keywords = {genomic stability{\textbar}mutation selection{\textbar}canalization{\textbar}fitness landscape{\textbar}quasispecies},
	pages = {1405--1409},
	file = {Full Text PDF:/Users/jagomez/Library/Application Support/Firefox/Profiles/4otj4fuq.default/zotero/storage/QM8QZEXN/Krakauer and Plotkin - 2002 - Redundancy, antiredundancy, and the robustness of .pdf:application/pdf;Snapshot:/Users/jagomez/Library/Application Support/Firefox/Profiles/4otj4fuq.default/zotero/storage/24CM52PQ/1405.html:text/html}
