	keywords = {Adult, Brain, Computational Biology, Computer Simulation, Female, Fractals, Humans, Least-Squares Analysis, Magnetoencephalography, Memory, Models, Neurological, Models, Statistical, Models, Theoretical, Multivariate Analysis, Neural Networks (Computer)},
