@article{kahneman_back_1997,
	title = {Back to {Bentham}? {Explorations} of {Experienced} {Utility}},
	volume = {112},
	issn = {0033-5533, 1531-4650},
	shorttitle = {Back to {Bentham}?},
	url = {http://qje.oxfordjournals.org/content/112/2/375},
	doi = {10.1162/003355397555235},
	abstract = {Two core meanings of “utility” are distinguished. “Decision utility” is the weight of an outcome in a decision. “Experienced utility” is hedonic quality, as in Bentham's usage. Experienced utility can be reported in real time (instant utility), or in retrospective evaluations of past episodes (remembered utility). Psychological research has documented systematic errors in retrospective evaluations, which can induce a preference for dominated options. We propose a formal normative theory of the total experienced utility of temporally extended outcomes. Measuring the experienced utility of outcomes permits tests of utility maximization and opens other Unes of empirical research.},
	language = {en},
	number = {2},
	urldate = {2015-08-21},
